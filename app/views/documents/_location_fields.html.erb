  <span class="input-form-label">Street 2 <%= f.text_field :street2, :size => 12, :class => 'input-form-input' %>
  <span class="input-form-label">Town <%= f.text_field :town, :size => 12, :class => 'input-form-input' %>
  <span class="input-form-label">County <%= f.text_field :county, :size => 12, :class => 'input-form-input' %>
  <span class="input-form-label">Country <%= f.text_field :country, :size => 12, :class => 'input-form-input' %>
  <span class="input-form-label"><%= link_to_remove_fields image_tag("deleteicon.png"), f %>
<br /><br />
</div>


