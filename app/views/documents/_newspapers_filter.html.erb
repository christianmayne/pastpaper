<%= form_for :search_newspapers, :url => search_newspapers_path, :method=>'get', :html => {:class=>"form-inline"} do |f| %>
	<%#= Day %>
	<%= f.select(:day, Array(1..31), :prompt => "Day", :type=>"text", :class=>"span1") %>
	<%= f.select(:month, Array(1..12), :prompt => "Month") %>
	<%= f.select(:year, Array(1870..1945), :prompt => "Year") %>
	<%= f.submit "Go",:class=>"btn btn-primary"%>
<%end%>
